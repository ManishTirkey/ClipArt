{
  "name": "clipart",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc && node ./scripts/copy-static.cjs",
    "dev": "concurrently \"tsc -w\" \"wait-on dist/preload.js && node ./scripts/copy-static.cjs --watch\" \"electron .\"",
    "start": "electron .",
    "typecheck": "tsc --noEmit",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@types/node": "^24.3.1",
    "concurrently": "^9.2.1",
    "electron": "^38.0.0",
    "electron-builder": "^26.0.12",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "wait-on": "^8.0.4"
  },
  "dependencies": {
    "electron-updater": "^6.6.2"
  }
}
